{
  "name": "basis",
  "version": "0.0.1",
  "description": "Complete project template & setup for production ready Node / ES6 web sites & apps",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/basis-stack/basis.git"
  },
  "keywords": [
    "Node",
    "ES6",
    "Template",
    "Production"
  ],
  "author": "Justin Ware",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basis-stack/basis/issues"
  },
  "homepage": "https://github.com/basis-stack/basis",
  "scripts": {
    "build": "eslint config/*.js gulpfile.babel.js && gulp",
    "start": "node dist/bin/start-basis",
    "dev": "cross-env NODE_ENV=development node dist/bin/start-basis",
    "prod": "cross-env NODE_ENV=production node dist/bin/start-basis",
    "test": "cross-env BABEL_ENV=test mocha packages/**/*Spec.js test/**/*Spec.js",
    "coverage": "cross-env BABEL_ENV=test nyc mocha packages/**/*Spec.js test/**/*Spec.js",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare:packages": "eslint gulpfile.prepare.js && gulp compile:packages --gulpfile gulpfile.prepare.js --require babel-register",
    "link:packages": "yarn prepare:packages && gulp link:packages",
    "unlink:packages": "gulp unlink:packages",
    "publish:packages": "gulp publish:packages"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "axios": "^0.18.0",
    "basis-client": "^0.0.17",
    "basis-components": "^0.0.14",
    "basis-server": "^0.0.26",
    "connected-react-router": "^4.5.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mock-require": "^3.0.2",
    "object-assign-deep": "^0.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "basis-build": "^0.0.31",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-basis-stack": "^0.0.19",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11",
    "gulp": "^3.9.1",
    "gulp-babel": "7",
    "gulp-eslint": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "mocha": "^5.2.0",
    "npm-check-updates": "^2.14.2",
    "nyc": "^13.0.1",
    "proxyquire": "^2.1.0",
    "react-test-renderer": "^16.4.2",
    "run-sequence": "^2.2.1",
    "sinon": "^6.1.5",
    "webpack": "^4.17.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
