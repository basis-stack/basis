{
  "name": "basis",
  "version": "0.0.1",
  "description": "Complete project template & setup for production ready Node / ES6 web sites & apps",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/basis-stack/basis.git"
  },
  "keywords": [
    "Node",
    "ES6",
    "Template",
    "Production"
  ],
  "author": "Justin Ware",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basis-stack/basis/issues"
  },
  "homepage": "https://github.com/basis-stack/basis",
  "scripts": {
    "build": "eslint config/*.js gulpfile.babel.js && gulp",
    "start": "node dist/bin/start-basis",
    "dev": "cross-env NODE_ENV=development node dist/bin/start-basis",
    "prod": "cross-env NODE_ENV=production node dist/bin/start-basis",
    "test": "cross-env BABEL_ENV=test mocha packages/**/*Spec.js test/**/*Spec.js",
    "coverage": "cross-env BABEL_ENV=test nyc mocha packages/**/*Spec.js test/**/*Spec.js",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare:packages": "eslint gulpfile.prepare.js && gulp prepare:packages --gulpfile gulpfile.prepare.js --require @babel/register",
    "link:packages": "yarn prepare:packages && gulp link:packages --gulpfile gulpfile.prepare.js --require @babel/register",
    "unlink:packages": "gulp unlink:packages",
    "publish:packages": "gulp publish:packages"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "basis-client": "^0.0.28",
    "basis-components": "^0.0.18",
    "basis-server": "^0.0.34",
    "connected-react-router": "^6.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "object-assign-deep": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-redux": "^v7.2.0",
    "react-router": "^5.2.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "basis-build": "^0.0.41",
    "basis-testing": "^0.0.7",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-basis-stack": "^0.0.28",
    "eslint-config-sinon": "^4.0.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "react-test-renderer": "^16.13.1",
    "rxjs": "^6.5.5",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
