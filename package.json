{
  "name": "basis",
  "version": "0.0.1",
  "description": "Complete project template & setup for production ready Node / ES6 web sites & apps",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/basis-stack/basis.git"
  },
  "keywords": [
    "Node",
    "ES6",
    "Template",
    "Production"
  ],
  "author": "Justin Ware",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basis-stack/basis/issues"
  },
  "homepage": "https://github.com/basis-stack/basis",
  "scripts": {
    "build": "eslint config/*.js gulpfile.babel.js && gulp",
    "start": "node dist/bin/start-basis",
    "dev": "cross-env NODE_ENV=development node dist/bin/start-basis",
    "prod": "cross-env NODE_ENV=production node dist/bin/start-basis",
    "test": "cross-env BABEL_ENV=test mocha packages/**/*Spec.js test/**/*Spec.js",
    "coverage": "cross-env BABEL_ENV=test nyc mocha packages/**/*Spec.js test/**/*Spec.js",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare:packages": "eslint gulpfile.prepare.js && gulp prepare:packages --gulpfile gulpfile.prepare.js --require @babel/register",
    "link:packages": "yarn prepare:packages && gulp link:packages --gulpfile gulpfile.prepare.js --require @babel/register",
    "unlink:packages": "gulp unlink:packages",
    "publish:packages": "gulp publish:packages"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "axios": "^0.19.0",
    "basis-client": "^0.0.25",
    "basis-components": "^0.0.16",
    "basis-server": "^0.0.31",
    "connected-react-router": "^6.5.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "object-assign-deep": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-jss": "^8.6.1",
    "react-redux": "^v7.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "basis-build": "^0.0.38",
    "basis-testing": "^0.0.6",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.4.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-basis-stack": "^0.0.27",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^1.4.0",
    "mocha": "^6.2.2",
    "npm-check-updates": "^3.1.25",
    "nyc": "^14.1.1",
    "react-test-renderer": "^16.11.0",
    "sinon": "^7.5.0",
    "webpack": "^4.41.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
